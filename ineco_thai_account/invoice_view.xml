            <field name="view_mode">calendar</field>
            <field name="view_id" ref="view_ineco_supplier_invoice_due_calendar" />
            <field name="act_window_id" ref="action_ineco_supplier_invoice_due"/>
        </record>
        
        <menuitem action="action_ineco_supplier_invoice_due" id="menu_ineco_supplier_invoice_due" parent="account.menu_finance_payables" sequence="110"/>

        
