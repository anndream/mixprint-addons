	        	    		</tree>
	        	    	</field>
	        	    </page>
	        	</page>
		        <field name="partner_id" position="after">
		        	<field name="cheque_id"  context="{'default_type':'out'}" domain="[('type','=','out'),('state','=','draft')]"/>
		        </field>	        	       		        	
			</field>
		</record>    

		<!-- Supplier Payment -->
