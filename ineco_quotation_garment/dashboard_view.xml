<?xml version="1.0"?>
<openerp>
     <data>
     
		<!-- Sale Summary Dashboard -->
        <record id="view_ineco_sale_summary_tree" model="ir.ui.view">
            <field name="name">ineco.sale.summary.tree</field>
            <field name="model">ineco.sale.summary</field>
            <field name="arch" type="xml">
                <tree string="Problem Type">
                    <field name="user_id" />
                    <field name="so" />
                    <field name="mo" />
                    <field name="lose" />
                    <field name="percent90" />
                    <field name="percent50" />
                    <field name="percent10" />
                </tree>
            </field>
        </record>		

        <record id="view_ineco_sale_summary_form" model="ir.ui.view">
            <field name="name">ineco.sale.summary.form</field>
            <field name="model">ineco.sale.summary</field>
            <field name="arch" type="xml">
                <form string="Problem Type">
                    <field name="user_id" />
                    <field name="so" />
                    <field name="mo" />
                    <field name="lose" />
                    <field name="percent10" />
                    <field name="percent50" />
                    <field name="percent90" />
                </form>
            </field>
        </record>		
        
        <record id="view_ineco_sale_summary_filter" model="ir.ui.view">
            <field name="name">ineco.sale.summary.select</field>
            <field name="model">ineco.sale.summary</field>
            <field name="arch" type="xml">
                <search string="Search Sale Summary">
                	<field name="user_id" />
               	</search>
            </field>
        </record>
        
        <record id="action_ineco_sale_summary" model="ir.actions.act_window">
            <field name="name">Sale Summary</field>
            <field name="res_model">ineco.sale.summary</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_ineco_sale_summary_filter"/>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Click to create a sale summary. 
              </p><p>
                This is the list of all problem type that have to be
                prepared, according to your different sales orders and your
                logistics rules.
              </p>
            </field>
        </record>
        


		<!-- Sale Summary Dashboard 4 -->
        <record id="view_ineco_sale_summary4_tree" model="ir.ui.view">
            <field name="name">ineco.sale.summary4.tree</field>
            <field name="model">ineco.sale.summary4</field>
            <field name="arch" type="xml">
                <tree string="Problem Type">
                    <field name="nickname" />
                    <field name="so1" sum="Total"/>
                    <field name="so2" sum="Total" string=""/>
                    <field name="mo1" sum="Total"/>
                    <field name="mo2" sum="Total" string=""/>
                    <field name="percent901" sum="Total"/>
                    <field name="percent902" sum="Total" string=""/>
                    <field name="percent501" sum="Total"/>
                    <field name="percent502" sum="Total" string=""/>
                    <field name="percent301" sum="Total"/>
                    <field name="percent302" sum="Total" string=""/>
                    <field name="percent101" sum="Total"/>
                    <field name="percent102" sum="Total" string=""/>
                    <field name="lose1" sum="Total"/>
                    <field name="lose2" sum="Total" string=""/>
                </tree>
            </field>
        </record>		
        
        <record id="view_ineco_sale_summary4_filter" model="ir.ui.view">
            <field name="name">ineco.sale.summary4.select</field>
            <field name="model">ineco.sale.summary4</field>
            <field name="arch" type="xml">
                <search string="Search Sale Summary">
                	<field name="user_id" />
                	<field name="nickname" />
               	</search>
            </field>
        </record>
        
        <record id="action_ineco_sale_summary4" model="ir.actions.act_window">
            <field name="name">Sale Summary 1</field>
            <field name="res_model">ineco.sale.summary4</field>
            <field name="type">ir.actions.act_window</field>
            <field name="view_type">form</field>
            <field name="view_mode">tree,form</field>
            <field name="search_view_id" ref="view_ineco_sale_summary4_filter"/>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                Click to create a sale summary. 
              </p><p>
                This is the list of all problem type that have to be
                prepared, according to your different sales orders and your
                logistics rules.
              </p>
            </field>
        </record>
                   
        <menuitem
        	id="menu_mixprint_dashboard"
        	parent="base.menu_sales"
        	name="Mixprint"
        	sequence="0" />
        	        
        <menuitem 
            id="menu_ineco_sale_summary"
        	action="action_ineco_sale_summary4" 
        	parent="menu_mixprint_dashboard"
        	sequence="1"/> 
        	
